diff -burN gcc-6.3.0.orig/config.sub gcc-6.3.0/config.sub
# patch by SP193 and uyjulian
--- gcc-6.3.0.orig/config.sub	2016-08-31 03:49:03.000000000 -0500
+++ gcc-6.3.0/config.sub	2017-01-07 14:05:58.000000000 -0600
@@ -770,6 +770,10 @@
 		basic_machine=i386-unknown
 		os=-vsta
 		;;
+	iop)
+		basic_machine=mipsel-scei
+		os=-irx
+		;;
 	iris | iris4d)
 		basic_machine=mips-sgi
 		case $os in
@@ -1399,7 +1403,8 @@
 	      | -morphos* | -superux* | -rtmk* | -rtmk-nova* | -windiss* \
 	      | -powermax* | -dnix* | -nx6 | -nx7 | -sei* | -dragonfly* \
 	      | -skyos* | -haiku* | -rdos* | -toppers* | -drops* | -es* \
-	      | -onefs* | -tirtos* | -phoenix*)
+	      | -onefs* | -tirtos* | -phoenix* \
+       | -irx*)
 	# Remember, each alternative MUST END IN *, to match a version number.
 		;;
 	-qnx*)
