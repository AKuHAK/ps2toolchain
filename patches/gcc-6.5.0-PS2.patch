diff -burN gcc-6.3.0.orig/gcc/config.gcc gcc-6.3.0/gcc/config.gcc
--- gcc-6.3.0.orig/gcc/config.gcc	2016-11-07 15:38:43.000000000 -0600
+++ gcc-6.3.0/gcc/config.gcc	2017-01-07 14:05:16.000000000 -0600
@@ -2218,6 +2218,11 @@
 	tmake_file="mips/t-elf"
 	tm_defines="${tm_defines} MIPS_ISA_DEFAULT=3 MIPS_ABI_DEFAULT=ABI_N32"
 	;;
+mipsel-*-irx*)
+	tm_file="dbxelf.h elfos.h newlib-stdint.h ${tm_file} mips/elf.h"
+	tmake_file="mips/t-elf mips/dbx.h"
+	target_cpu_default=MASK_SOFT_FLOAT_ABI
+	;;
 mips64-*-elf* | mips64el-*-elf*)
 	tm_file="elfos.h newlib-stdint.h ${tm_file} mips/elf.h"
 	tmake_file="mips/t-elf"
diff -burN gcc-6.3.0.orig/libgcc/config.host gcc-6.3.0/libgcc/config.host
